{
  "name": "gatsby-source-directus",
  "version": "0.0.44",
  "description": "Source plugin for Gatsby to fetch data from Directus Headless CMS",
  "main": "gatsby-node.js",
  "files": [
    "index.js",
    "fetch.js",
    "gatsby-node.js",
    "process.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iKonrad/gatsby-source-directus.git"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "bugs": {
    "url": "https://github.com/iKonrad/gatsby-source-directus/issues"
  },
  "author": "Konrad Jarosinski <konrad@jarosinski.uk>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "directus-sdk-javascript": "^2.5.1",
    "gatsby-source-filesystem": "^2.0.1-beta.7",
    "gatsby-node-helpers": "^0.1.3",
    "lodash": "^4.17.5",
    "pluralize": "^7.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "colors": "^1.1.2",
    "cross-env": "^5.0.5"
  }
}
